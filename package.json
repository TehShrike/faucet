{
	"name": "@tehshrike/faucet",
	"version": "0.0.4",
	"description": "human-readable TAP summarizer",
	"main": "index.js",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"faucet": "bin/cmd.js"
	},
	"sideEffects": false,
	"scripts": {
		"lint": "eslint --ext=js,mjs .",
		"pretest": "npm run lint",
		"tests-only": "tape test/**/*.js | node bin/cmd",
		"test": "npm run tests-only"
	},
	"dependencies": {
		"defined": "^1.0.1",
		"duplexer": "^0.1.2",
		"minimist": "^1.2.7",
		"readable-stream": "^2.3.7",
		"safe-regex-test": "^1.0.0",
		"sprintf-js": "^1.1.2",
		"tap-parser": "^0.7.0",
		"through2": "^0.2.3"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/substack/faucet.git"
	},
	"homepage": "https://github.com/substack/faucet",
	"keywords": [
		"tap",
		"human",
		"test",
		"anything",
		"protocol",
		"pretty"
	],
	"author": {
		"name": "James Halliday",
		"email": "mail@substack.net",
		"url": "http://substack.net"
	},
	"funding": {
		"url": "https://github.com/sponsors/ljharb"
	},
	"license": "MIT",
	"devDependencies": {
		"@ljharb/eslint-config": "^21.0.1",
		"auto-changelog": "^2.4.0",
		"eslint": "=8.8.0"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true
	},
	"publishConfig": {
		"ignore": [
			".github/workflows",
			"images/",
			"example/"
		]
	}
}
